LOAD CSV
     FROM INLINE (id, number [null if '\N'], data)
     INTO postgresql://yugabyte:yugabyte@localhost:5433/pgloader?nullif

     BEFORE LOAD DO
      $$ drop table if exists nullif; $$,
      $$ CREATE TABLE nullif
         (
            id     serial primary key,
            number integer,
            data   text
         );
      $$

     WITH fields terminated by ',',
          fields enclosed by '"',
          fields escaped by backslash-quote;


"1",\N,"testing nulls"
"2","2","another test"
